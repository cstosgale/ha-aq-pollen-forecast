{
  "dockerComposeFile": "docker-compose.yaml",
  "service": "hadev",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.pylint",
        "ms-python.autopep8"
      ],
    "settings": {
      "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8",
        "editor.formatOnSave": true
      }
    }
    }
  }
}